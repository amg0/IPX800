<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
    <specVersion>
        <major>1</major>
        <minor>0</minor>
    </specVersion>
    <serviceStateTable>
        <stateVariable sendEvents="yes">
            <name>Present</name>
            <dataType>boolean</dataType>
            <defaultValue>0</defaultValue>
            <shortCode>present</shortCode>
        </stateVariable>
    </serviceStateTable>
    <actionList>
        <action>
            <name>Refresh</name>
        </action>
        <action>
            <name>IPXNames</name>
        </action>
        <action>
            <name>GetPresent</name>
            <argumentList>
                <argument>
                    <name>PresentStatus</name>
                    <direction>out</direction>
                    <relatedStateVariable>Present</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>SetDebug</name>
            <argumentList>
                <argument>
                    <name>newDebugMode</name>
                    <direction>in</direction>
                    <relatedStateVariable>Debug</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
	</actionList>
</scpd>
